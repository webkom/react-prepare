    "@types/react-redux": "^7.1.23",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.29.4",
    "koa": "^2.13.4",
    "mocha": "^9.2.2",
    "node-fetch": "^2.6.7",
    "nyc": "^15.1.0",
    "prettier": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^5.1.2",
    "redux": "^4.1.2",
