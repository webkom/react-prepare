{
  "name": "@webkom/react-prepare",
  "version": "0.8.0",
  "description": "Prepare you app state for async server-side rendering and more!",
  "main": "dist/index.js",
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint src/**/*.js",
    "lint:prettier": "prettier '**/*.{js,css,md,ts,tsx}' --check",
    "build": "babel src -d dist --source-maps --extensions '.js'",
    "test": "node_modules/.bin/mocha --compilers js:babel-register src/**/*.spec.js",
    "make": "yarn run lint && yarn run clean && yarn run build && yarn run test",
    "prettier": "prettier '**/*.{js,css,md,ts,tsx,json}' --write"
  },
  "repository": {
    "url": "git@github.com:elierotenberg/react-prepare",
    "type": "git"
  },
  "author": "Elie Rotenberg <elie@rotenberg.io>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "deep-equal": "^2.0.5",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.29.4",
    "koa": "^2.13.4",
    "mocha": "^3.2.0",
    "node-fetch": "^1.6.3",
    "prettier": "^2.6.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^6.1.5",
    "source-map-support": "^0.4.5",
    "tcomb": "^3.2.15"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.2"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.17.8"
  }
}
